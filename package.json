{
  "name": "brainlan-www",
  "description": "Brainlan website",
  "private": false,
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && astro build",
    "build:node": "astro build --node",
    "preview": "astro preview --node",
    "astro": "astro",
    "format": "prettier -w .",
    "lint:eslint": "eslint . --ext .js,.ts,.astro",
    "update": "rm -fr node_modules package-lock.json && npm outdated | awk 'NR>1 {print $1\"@\"$4}' | xargs npm install && npm update --save"
  },
  "dependencies": {
    "@astrojs/check": "^0.5.5",
    "@astrojs/node": "^8.2.0",
    "@astrojs/sitemap": "^3.1.1",
    "@astrojs/tailwind": "^5.1.0",
    "@astrojs/vercel": "^7.3.1",
    "@fontsource-variable/inter": "^5.0.16",
    "@vercel/analytics": "^1.2.2",
    "astro": "^4.4.2",
    "flowbite": "^2.3.0",
    "flowbite-typography": "^1.0.3",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "prettier": "^3.2.5",
    "prettier-plugin-astro": "^0.13.0",
    "prettier-plugin-tailwindcss": "^0.5.11"
  }
}
