{
  "name": "brainlan-www",
  "description": "Brainlan website",
  "private": false,
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && astro build",
    "build:node": "astro build --node",
    "preview": "astro preview --node",
    "astro": "astro",
    "format": "prettier -w .",
    "lint:eslint": "eslint . --ext .js,.ts,.astro",
    "update": "rm -fr node_modules package-lock.json && npm outdated | awk 'NR>1 {print $1\"@\"$4}' | xargs npm install && npm update --save"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.0",
    "@astrojs/node": "^8.3.2",
    "@astrojs/sitemap": "^3.1.6",
    "@astrojs/tailwind": "^5.1.0",
    "@astrojs/vercel": "^7.7.2",
    "@fontsource-variable/inter": "^5.0.19",
    "@vercel/analytics": "^1.3.1",
    "astro": "^4.12.3",
    "flowbite": "^2.4.1",
    "flowbite-typography": "^1.0.3",
    "tailwindcss": "^3.4.7",
    "typescript": "^5.5.4"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "prettier": "^3.3.3",
    "prettier-plugin-astro": "^0.13.0",
    "prettier-plugin-tailwindcss": "^0.5.14"
  }
}
